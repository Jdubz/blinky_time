{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "env": {}
    },
    "blinky-serial": {
      "type": "stdio",
      "command": "node",
      "args": [
        "blinky-serial-mcp/dist/index.js"
      ],
      "env": {}
    }
  }
}